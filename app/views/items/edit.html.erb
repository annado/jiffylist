  <p>
    <%= f.check_box :checked %>
    <%= f.text_field :title %>
    <%= f.submit 'Update' %>
  </p>
  <p>
  </p>
  <p>
  </p>
<% end %>

<div class="ft">Back to <%= link_to @item.list.name, '/list/' + @item.list.hash_id %></div>